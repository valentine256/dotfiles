{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "http://*.naver.com/*", "http://*.nhncorp.com/*", "http://*.navercorp.com/*", "https://*.naver.com/*", "https://*.nhncorp.com/*", "https://*.navercorp.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "네이버 동영상 서비스 이용을 위한 확장 프로그램",
   "externally_connectable": {
      "matches": [ "http://*.naver.com/*", "http://*.nhncorp.com/*", "http://*.navercorp.com/*", "https://*.naver.com/*", "https://*.nhncorp.com/*", "https://*.navercorp.com/*" ]
   },
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "네이버 동영상 플러그인",
   "permissions": [ "activeTab", "nativeMessaging", "http://*.naver.com/*", "http://*.nhncorp.com/*", "http://*.navercorp.com/*", "https://*.naver.com/*", "https://*.nhncorp.com/*", "https://*.navercorp.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1.8"
}
